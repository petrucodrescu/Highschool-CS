#include <iostream>
using namespace std;

int main() {

  //..create a 2d array of 3 rows and 2 columns
  int n[3][2] = {{1,2},
                 {3,4},
                 {5,6}};

  for(int row = 0; row < 3; row++)
    {
      for(int col = 0; col < 2; col++)
        cout << n[row][col];
      cout << endl;
    }//..end for row
}
