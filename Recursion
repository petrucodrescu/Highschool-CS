#include <iostream>
using namespace std;

//..recursive countDown
void countDown(int n)
{
  //..base case
  if(n == 0)
  {
    cout << "Blast off!" << endl;
  }
  //..recursive call
  else
  {
    cout << n << endl;
    countDown(n - 1);
  }
}
