void maxMin(int a, int b, int &max, int &min)
{
  if(a > b)
  {
    max = a;
    min = b;
  }

  else if(b > a)
  {
    max = b;
    min = a;
  }

  cout << max << endl;
  cout << min << endl;
}

int main(){
  int a = 10;
  int b = 3;
  int max, min;
  maxMin(a, b, max, min);
  
}
