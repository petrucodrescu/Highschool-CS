#include <iostream>
#include <stack>
using namespace std;

int main() {
  stack<int> st;
  cout << "Size: " << st.size() << endl;
  if(st.empty())
  {
    cout << "Stack is empty" << endl;
  }

  st.push(1);
  st.push(2);
  st.push(3);
  st.push(4);
  st.push(5);
  st.push(6);
  st.push(7);
  st.push(8);
  st.push(9);
  st.push(10);

  while(!st.empty())
    {
      cout << st.top() << endl;
      st.pop();
    }
}
