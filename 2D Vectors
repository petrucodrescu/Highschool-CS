#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
  vector<vector<int>> grid(4);
  vector<int> row1{1,2,3,4};
  vector<int> row2{5,6,7,8};
  vector<int> row3{9,10,11,12};
  vector<int> row4{13,14,15,16};

  for(int i = 0; i < grid.size(); i++)
    {
      for(int j = 0; j < grid[i].size(); j++)
        cout << grid[i][j] << " ";
      cout << endl;
    }
}
